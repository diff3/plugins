# Script name can be the called name, or something more meaningful.
SCRIPTNAME=$0
PATH=/usr/local/bin:/usr/bin:/bin:/sbin

. /etc/init.d/init_utilities

start_function() {

    # set overcommitment mode 2
    #echo "2" >/proc/sys/vm/overcommit_memory
    # set overcommitment ratio 100% - no overcommitment
    #echo "100" >/proc/sys/vm/overcommit_ratio

    # mknod for SATA CDROM devices
    #rm -f /dev/scd0 /dev/scd1
    #try_command mknod /dev/scd0 b 11 0
    #try_command mknod /dev/scd1 b 11 1
    return 0
}

stop_function() {

    # remove SATA CDROM dev nodes
    #rm -f /dev/scd0 /dev/scd1
    return 0
}

case $1 in
    "start")
    start_function
    ;;
    "stop")
    stop_function
    ;;
    "restart")
    stop_function
    start_function
    ;;
    *)
    echo "Usage: $0 {start|stop|restart}"
esac
